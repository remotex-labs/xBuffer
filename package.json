{
    "name": "@remotex-labs/xbuffer",
    "main": "dist/cjs/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "module": "./dist/esm/index.js",
    "author": "Garefild",
    "version": "v1.0.0-local",
    "license": "Mozilla Public License Version 2.0",
    "description": "A library with Node.js Buffer api for the browser",
    "homepage": "https://github.com/remotex-lab/xStruct",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/remotex-lab/xBuffer.git"
    },
    "bugs": {
        "url": "https://github.com/remotex-lab/xBuffer/issues"
    },
    "keywords": [
        "xbuffer",
        "xBuffer",
        "buffer",
        "Uint8Array",
        "dataview",
        "arraybuffer",
        "browser",
        "compatible"
    ],
    "engines": {
        "node": ">=20"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/esm/index.js"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/cjs/index.js"
            }
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "dev": "xBuild -w",
        "test": "jest",
        "build": "xBuild",
        "test:coverage": "jest --coverage",
        "lint": "xbuild --tc && eslint . -c ./eslint.config.mjs",
        "ci:test": "jest",
        "ci:clean": "rm -rf ./dist",
        "ci:build": "npm run build"
    },
    "devDependencies": {
        "jest": "^30.0.5",
        "eslint": "^9.16.0",
        "typescript-eslint": "^8.16.0",
        "eslint-plugin-jsdoc": "^50.6.0",
        "@swc/jest": "^0.2.36",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.10.1",
        "@remotex-labs/xbuild": "^1.3.6"
    },
    "dependencies": {
    }
}
